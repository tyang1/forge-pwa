files:
   "/etc/nginx/conf.d/01_proxy.conf":
     mode: "00644"
     owner: root
     group: root
     content: |
       map $http_upgrade $connection_upgrade {
         default        "upgrade";
         ""            "";
       }
 
 server {
        listen         80;
        server_name    www.forgepwa.com;
        return         301 https://$server_name$request_uri;
 } 
